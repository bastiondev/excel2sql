{
  "name": "excel2sql",
  "version": "0.1.0",
  "description": "A TypeScript library for translating Excel workbooks to and from SQL queries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "build:browser": "webpack --config webpack.browser.config.js",
    "test": "jest",
    "prepare:browser-tests": "node test/browser/prepare-test-data.js",
    "test:browser": "npm run prepare:browser-tests && karma start",
    "prepublishOnly": "npm run build && npm run build:browser",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "excel",
    "sql",
    "typescript",
    "browser"
  ],
  "author": "adam@bastion.dev",
  "license": "MIT",
  "dependencies": {
    "exceljs": "^4.4.0",
    "sql.js": "^1.13.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/exceljs": "^1.3.2",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.15.24",
    "chai": "^4.3.10",
    "jest": "^29.7.0",
    "karma": "^6.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
